<AnchorPane prefHeight="294.0" prefWidth="389.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.UpdateLoginController">
   <children>
      <ImageView fitHeight="336.0" fitWidth="457.0" layoutX="-68.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../img4/50.jpg" />
         </image>
      </ImageView>
      <Label fx:id="UpdateLevelLbl" layoutX="49.0" layoutY="73.0" text="Level" textFill="#f2eded">
         <font>
            <Font name="System Bold" size="16.0" />
         </font></Label>
      <Button fx:id="btn20" layoutX="257.0" layoutY="248.0" mnemonicParsing="false" onAction="#UpdateLoginOk" prefHeight="25.0" prefWidth="52.0" text="OK" />
      <Button fx:id="btn21" layoutX="315.0" layoutY="248.0" mnemonicParsing="false" onAction="#CloseAppUpdateLogin" text="Cancel" />
      <Label fx:id="StatusLbl" layoutX="14.0" layoutY="206.0" prefHeight="27.0" prefWidth="296.0" text="     Status" textFill="#f20404">
         <font>
            <Font size="18.0" />
